{
  "expo": {
    "name": "VisionAsistidaApp",
    "slug": "VisionAsistidaApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "CAMERA",
        "RECORD_AUDIO"
      ],
      "package": "com.dana.VisionAsistida"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
     "android": {
  "scheme": "visionasistida"
}

  }
}
